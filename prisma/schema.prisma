generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(cuid())
  image         String?
  name          String
  office        String
  dateNaissance DateTime
  rg            String
  adress        String
  ctps          String
  issuingBody   String
  number        String
  cpf           String   @unique
  cep           String
  complement    String
  manager       Boolean  @default(false)
}
